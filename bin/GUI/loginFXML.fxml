<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="413.0" prefWidth="312.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.loginFXMLController">
	<children>
		<AnchorPane layoutX="-7.0" prefHeight="414.0" prefWidth="326.0">
			<children>
				<Label layoutX="91.0" layoutY="97.0" text="Please Login to Continue.">
					<font>
						<Font name="18thCentury" size="17.0" />
					</font>
				</Label>
				<Label layoutX="25.0" layoutY="27.0" prefHeight="34.0" prefWidth="275.0" text="Welcome to ReviewEat" underline="true">
					<font>
						<Font name="18thCentury" size="33.0" />
					</font>
				</Label>
				<Label layoutX="42.0" layoutY="135.0" text="Username:">
					<font>
						<Font name="18thCentury" size="20.0" />
					</font>
				</Label>
				<Label layoutX="42.0" layoutY="192.0" text="Password:">
					<font>
						<Font name="18thCentury" size="20.0" />
					</font>
				</Label>
				<TextField fx:id="usernameField" layoutX="125.0" layoutY="131.0" promptText="Username" />
				<PasswordField fx:id="passwordField" layoutX="125.0" layoutY="188.0" promptText="Password" />
				<Button fx:id="registerButton" alignment="CENTER" layoutX="42.0" layoutY="325.0" mnemonicParsing="false" onAction="#register" text="Register Here">
					<font>
						<Font name="18thCentury" size="13.0" />
					</font>
				</Button>
				<Button fx:id="updatePasswordButton" layoutX="178.0" layoutY="327.0" mnemonicParsing="false" onAction="#updatePassword" text="Update Password">
					<font>
						<Font name="18thCentury" size="12.0" />
					</font>
				</Button>
				<Button fx:id="loginButton" layoutX="126.0" layoutY="236.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="72.0" text="Login">
					<font>
						<Font name="18thCentury" size="16.0" />
					</font>
				</Button>
				<Button fx:id="deleteAccountButton" layoutX="178.0" layoutY="359.0" mnemonicParsing="false" onAction="#deleteAccount" text="Delete Account">
					<font>
						<Font name="18thCentury" size="12.0" />
					</font>
				</Button>
				<Label layoutX="42.0" layoutY="302.0" text="New to the system? ">
					<font>
						<Font name="18thCentury" size="14.0" />
					</font>
				</Label>
				<Separator layoutX="32.0" layoutY="290.0" prefHeight="2.0" prefWidth="261.0" />
				<Label layoutX="163.0" layoutY="302.0" text="Looking for something else?">
					<font>
						<Font name="18thCentury" size="14.0" />
					</font>
				</Label>
